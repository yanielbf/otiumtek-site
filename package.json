{
  "name": "otiumtek-website",
  "version": "1.2.0",
  "description": "Otiumtek website",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "dotenv -e .env -- bun run dev:services && next dev --turbopack",
    "dev:next": "next dev --turbopack",
    "dev:services": "docker compose up -d",
    "devsafe": "rm -rf .next && next dev",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "payload": "payload",
    "start": "next start",
    "services:start": "docker compose up -d",
    "services:stop": "docker compose down",
    "services:logs": "docker compose logs -f",
    "services:reset": "docker compose down -v && docker compose up -d",
    "services:status": "docker compose ps",
    "email:start": "docker compose up -d inbucket",
    "email:stop": "docker compose stop inbucket",
    "email:logs": "docker compose logs -f inbucket",
    "email:test": "bun run src/scripts/test-email.ts",
    "s3:start": "docker compose up -d minio",
    "s3:stop": "docker compose stop minio",
    "s3:logs": "docker compose logs -f minio",
    "s3:console": "open http://localhost:9002",
    "db:start": "docker compose up -d postgres",
    "db:stop": "docker compose stop postgres",
    "db:logs": "docker compose logs -f postgres",
    "db:reset": "docker compose down -v postgres && docker compose up -d postgres",
    "db:connect": "docker exec -it acme-postgres psql -U postgres -d acme-website"
  },
  "dependencies": {
    "@daveyplate/better-auth-ui": "^3.1.5",
    "@hookform/resolvers": "^5.2.2",
    "@marsidev/react-turnstile": "^1.3.1",
    "@payloadcms/db-postgres": "3.54.0",
    "@payloadcms/db-sqlite": "3.54.0",
    "@payloadcms/email-nodemailer": "3.54.0",
    "@payloadcms/email-resend": "3.54.0",
    "@payloadcms/live-preview-react": "3.54.0",
    "@payloadcms/next": "3.54.0",
    "@payloadcms/plugin-import-export": "3.54.0",
    "@payloadcms/plugin-seo": "3.54.0",
    "@payloadcms/richtext-lexical": "3.54.0",
    "@payloadcms/storage-s3": "3.54.0",
    "@payloadcms/ui": "3.54.0",
    "@react-email/components": "^0.5.1",
    "@react-email/render": "^1.2.1",
    "@vercel/analytics": "^1.5.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "better-auth": "^1.3.7",
    "better-auth-harmony": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^10.0.0",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "html-react-parser": "^5.2.6",
    "lucide-react": "^0.542.0",
    "motion": "^12.23.12",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "payload": "3.54.0",
    "payload-auth": "^1.6.4",
    "radix-ui": "^1.4.3",
    "react": "19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "react-medium-image-zoom": "^5.3.0",
    "resend": "^6.0.1",
    "sharp": "0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "5.9.2",
    "zod": "^4.1.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
